<p>âœ… Debugging selected_conference_proceedings.liquid</p>
<pre>{{ data | jsonify }}</pre>

<ul class="card-text font-weight-light list-group list-group-flush">
  {% for content in data %}
    <li class="list-group-item">
      <div class="row">
        <div class="col-xs-2 col-sm-2 col-md-2 text-center date-column">
          <span class="badge font-weight-bold danger-color-dark text-uppercase align-middle" style="min-width: 75px"> 
            {{ content.date }} 
          </span>
        </div>
        <div class="col-xs-10 col-sm-10 col-md-10 mt-2 mt-md-0">
          <h6 class="title font-weight-bold ml-1 ml-md-4">
            {{ content.title }}
          </h6>
          <h6 class="ml-1 ml-md-4" style="font-size: 0.95rem">{{ content.conference }}</h6>
          <h6 class="ml-1 ml-md-4" style="font-size: 0.95rem">{{ content.location }}</h6>

          {% if content.summary and content.summary != '' %}
            <ul class="ml-1 ml-md-4">
              <li>{{ content.summary }}</li>
            </ul>
          {% endif %}
        </div>
      </div>
    </li>
  {% endfor %}
</ul>

